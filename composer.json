{
    "name": "apb/user-bundle",
    "version": "1.3",
    "description": "Bundle to create user",
    "type": "symfony-bundle",
    "license": "MIT",
    "autoload": {
        "psr-4": {
            "Apb\\UserBundle\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Apb\\UserBundle\\Tests\\": "tests/"
        }
    },
    "authors": [
        {
            "name": "aurelien piquet"
        }
    ],
    "require": {
        "php": ">=8.2",
        "friendsofsymfony/rest-bundle": ">=3.3",
        "doctrine/orm": ">=2.2",
        "nelmio/api-doc-bundle": ">=4.11",
        "league/oauth2-server-bundle": "*",
        "apb/mailer-bundle": "*",
        "lexik/jwt-authentication-bundle": "^3.0"
    },
    "extra": {
        "symfony": {
            "endpoint": [
                "https://api.github.com/repos/aurelienpiquet/user-bundle/contents/index.json",
                "flex://defaults"
            ]
        }
    }
}
